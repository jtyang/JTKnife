<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin">

    <Button
        android:id="@+id/feature_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Feature"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/swiperefresh_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="SwipeRefreshRecyclerView"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/liveroom_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="LiveRoom"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/stackblur_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="stackblur"
        android:textAllCaps="false" />

    <Button
        android:id="@+id/banner_btn"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="banner"
        android:textAllCaps="false" />

</LinearLayout>
